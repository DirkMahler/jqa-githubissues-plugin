<jqa-plugin:jqassistant-plugin
        xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.1"
        name="GitHub-Issues">
    <description>Provides a scanner for GitHub Issues.</description>
    <model>
        <class>model.GitHub</class>
        <class>model.GitHubMilestone</class>
        <class>model.GitHubIssue</class>
        <class>model.GitHubRepository</class>
        <class>model.GitHubLabel</class>
        <class>model.GitHubUser</class>
        <class>model.GitHubComment</class>
        <class>model.GitHubCommit</class>
        <class>model.GitHubPullRequest</class>
        <class>model.GitHubMarkdownPointer</class>
    </model>
    <scanner>
        <class>scanner.GitHubIssueScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>